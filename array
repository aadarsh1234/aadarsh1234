#include <stdio.h>

int main()
{
    int rollno[5];
    int i;
    printf("enter the value of array");
    for( int i=0;i<=4;i++){
        scanf("%d",&rollno[i]);
        
    }
    for(i=0;i<=4;i++){
    printf("%d\n",rollno[i]);
    }
    return 0;
}

(4) reverse array

#include <stdio.h>

int main()
{
    int arr[5];
    for(int i=0;i<=4;i++){
    printf(" enter the element number %d\n",i+1);
    scanf("%d",&arr[i]);
    }
    for(int i=4;i>0;i--){
    printf("%d",arr[i]);
}
    return 0;
}




(2) print12345

#include <stdio.h>

int main()
{
    int arr[5];
    for(int i=0;i<=4;i++){
    printf(" \nenter the element number %d",i+1);
    scanf("%d",&arr[i]);
    }
    for(int i=0;i<5;i++){
    printf("%d",arr[i]);
}
    return 0;
}

(3) print 2
#include <stdio.h>

int main()
{
    int arr[5];
    for(int i=0;i<=4;i++){
    printf(" \nenter the element number %d",i+1);
    scanf("%d",&arr[i]);
    }
    
    printf("%d",arr[2]);

    return 0;
}

(4)
product of all elemet

#include <stdio.h>

int main()
{
    int n,i;
   int   product =1;
    printf("enter the value of array");
    scanf("%d",&n);
    int arr[n];
    printf("enter the %d element \n",n );
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
        
    }
    for(int i=0;i<n;i++){
        product *=arr[i];
        
    }
    printf("the product of all element is %d\n", product);

