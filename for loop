#include <stdio.h>

int main() {
    int n, factorial = 1;
  
    for (int i = 1; i <= 5; i++) {
        factorial *= i;
    
    printf(" %d\n", factorial);
    }
    return 0;
}
#include <stdio.h>

int main()
{
    int n;
    printf("enter the number");
    scanf("%d",&n);
    for(int i =1;i<=10;i++){
        printf("%d x %d =%d\n",n,i,n*i);
    }
    

    return 0;
}

#include <stdio.h>

int main() {
  
    for (int i = 1; i <= 100; i++) 
    {
        if(i%2==0 )
        printf("%d ", i);
    }
    return 0;
}