#include <stdio.h>

int main()
{
    int n;
    printf("Hello World:");
    scanf("%d",&n);
   int  a=0;
   for(int i=1;i<=n-1;i++){
       if(n%2==0)
       a=1;
       break;
   }
   if(n==1) printf("1 is neither composite and nor prime\n");
   else if(a==0)printf("the given number is prime \n");
   else printf("the given number is composite \n");
    return 0;
}