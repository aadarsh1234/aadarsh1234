#i#include <stdio.h>

int main()
{int radius,length,width,side,circle,rectangle,square;
printf("Enter radius:");
scanf("%d",&radius);
float π=3.1415;
circle=π*radius*radius;
printf("area for circle %d",circle);
printf("\nenter the length & width:");
scanf("%d%d",&length,&width); 
rectangle=length*width;
printf("area of rectangle:%d", rectangle );
printf("\nEnter side:");
scanf("%d%d",&side,&side);
square=side*side;
printf("area of square:%d", square);

    return 0;}
#include <stdio.h>
 int main ()
 {int a,b, add,sub,mul,division;
 printf("enter any two value");
 scanf("%d%d",&a,&b);
 add=a+b;
 printf("adding two value:%d",add);
 printf("\n");
 scanf("%d%d",&a,&b);
 sub=a-b;
 printf("sub two value:%d",sub);
 printf("mul any two value");
 scanf("%d%d",&a,&b);
 mul=a*b;
 printf("%d",mul);
 return 0;
 }

volume of sphere 
#include <stdio.h>

int main()
{
    int r=5;
    float v =4*3.1415*r*r*r/3;
    printf("%f",v);

    return 0;
}
