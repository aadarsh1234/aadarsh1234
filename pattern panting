int main()
{
 int n;
    printf("enter number of row");
    scanf("%d",&n);
    for(int i=1;i<=n; i++){
    for(int i=1;i<=n;i++){
        printf("%d ",i);
    }
    printf("\n");   
    }
    
    return 0;
}

