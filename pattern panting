#include <stdio.h>

int main()
{
 int n;
    printf("enter number of row");
    scanf("%d",&n);
    for(int i=1;i<=n; i++){
    for(int i=1;i<=n;i++){
        printf("%d ",i);
    }
    printf("\n");   
    }
    
    return 0;
}

(3)****
   ***
   **
   *
#include <stdio.h>

int main()
{
    int n;
    printf("enter the row");
    scanf("%d",&n);
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n+1-i;j++){
            printf("*");
        }
        printf("\n");
    }

    return 0;
}

