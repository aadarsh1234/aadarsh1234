#include <stdio.h>

int main()
{
    int n;
    printf("Hello World");
    scanf("%d",&n);
  
    for(int i=1;i<=n;i++){
    int a=1;
        for(int j=1;j<=n-i;j++){
            printf(" ");
        }
        for(int k=1;k<=i;k++){
           int d=a+64;
            char ch=(char)d;
            printf("%c",ch);
            a++;
        }
        printf("\n");
    }

    return 0;
}
(15)
   * 
  *** 
 *****
*******
include <stdio.h>

int main()
{
    int n;
    printf("Hello World");
    scanf("%d",&n);
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n-i;j++){
            printf(" ");
        }
        for(int k=1;k<=2*i-1;k++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
(16)
   A
