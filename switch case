#include <stdio.h>

int main()
{
    char operator;
    printf("enter the operator");
    scanf("%c",&operator);
    switch(operator)
    {
        case '1':printf("+");
        break;
        case '2':printf(" -");
        break;
        case '3':printf("*");
        break;
        case '4':printf("/");
        break;
        case '5':printf("mod");
        break;
        case '6': printf("no opretor ");
        break;
    }
    

    return 0;
}