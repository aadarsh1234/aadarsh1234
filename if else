
#include <stdio.h>

int main()
{
    int n;
    printf("enter the no");
    scanf("%d",&n);
    if(n%5==0 || n%3==0)
    {
        printf("it is divisible by 5 or 3" );
    }
    else
    {
        printf("it is not divisib by 5 or 3");
    }
    return 0;
}include <stdio.h>

int main()
{
    int a;
    printf("enter the value");
    scanf("%d",&a);
    if(a%5==0)  
    {
        printf("divided by 5 ");
    }
    if(a%11==0)
         printf(" divided by 11");
    return 0;
}
#include <stdio.h>

int main()
{
    int n;
    printf("enter the no");
    scanf("%d",&n);
    if(n>99 && n<1000)
    {
        printf("it is a three digit no" );
    }
    else
    {
        printf("it is not a three no");
    }
    return 0;
}